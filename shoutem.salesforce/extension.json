{
  "name": "salesforce",
  "title": "Salesforce MC",
  "version": "2.1.1",
  "description": "Salesforce Marketing Cloud",
  "platform": ">=9.0.1",
  "nativeChanges": {
    "lastVersion": "2.0.0",
    "settingsKeys": [
      "appId",
      "accessToken",
      "appEndpoint",
      "fcmSenderId"
    ]
  },
  "releaseNotes": "* No user facing changes\n* Optimized swagger usage to reduce download size when building apps",
  "dependencies": [
    "shoutem.application",
    "shoutem.auth",
    "shoutem.firebase",
    "shoutem.preview"
  ],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "settings": {
    "clientId": null,
    "authBaseUri": null,
    "restBaseUri": null,
    "salesforceAuthorized": false,
    "appId": null,
    "accessToken": null,
    "appEndpoint": null,
    "fcmSenderId": null
  },
  "pages": [
    {
      "name": "SalesforceSettingsPage",
      "type": "react-page"
    }
  ],
  "settingsPages": [
    {
      "page": "@.SalesforceSettingsPage",
      "title": "Settings"
    }
  ],
  "cloud": [
    {
      "type": "api",
      "script": "npm run migrate:docker && npm run start:server:docker"
    }
  ]
}
