{
  "name": "shoutem.interactive-faq",
  "version": "7.1.1",
  "description": "Make your FAQ screen fun and more user-friendly with this chatbot-like experience",
  "author": "Shoutem",
  "scripts": {
    "lint": "eslint --no-eslintrc -c .eslintrc src/**/*.{js,jsx}",
    "clean": "rimraf ./build/*",
    "build": "npm run clean && webpack --config ./bin/webpack.config.js",
    "watch": "npm run clean && webpack --watch --config ./bin/webpack.config.js",
    "serve": "webpack-dev-server --config ./bin/webpack.config.js",
    "dev": "run-p watch serve"
  },
  "dependencies": {
    "i18next": "^19.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@shoutem/eslint-config-react": "^1.0.1",
    "@shoutem/webpack-prepend-append": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "^7.21.5",
    "lodash": "^4.17.15",
    "sass": "1.54.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "10.2.0",
    "style-loader": "^1.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "terser-webpack-plugin": "4.2.3",
    "url-loader": "^3.0.0",
    "webpack": "4.47.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-modules-commonjs"
    ]
  }
}
