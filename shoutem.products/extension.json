{
  "name": "products",
  "version": "2.4.2-rc.1",
  "platform": "2.4.*",
  "title": "Products",
  "description": "Show products with purchase link",
  "categories": ["commerce"],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "icon": "server/assets/add-products-image.png",
  "dependencies": ["shoutem.cms"],
  "shortcuts": [
    {
      "name": "openProductsList",
      "title": "Products",
      "screen": "@.ProductsList",
      "icon": "theme://cart.png",
      "adminPages": [
        {
          "page": "shoutem.cms.CmsPage",
          "title": "Content",
          "parameters": {
            "schema": "@.Products"
          }
        }
      ],
      "settings": {
        "defaultShortcutTitle": "Products",
        "extensionTitle": "Products"
      }
    }
  ],
  "screens": [
    {
      "name": "ProductsList"
    },
    {
      "name": "ProductDetails"
    }
  ],
  "dataSchemas": [
    {
      "name": "Products",
      "path": "./server/data-schemas/Products.json"
    }
  ]
}
