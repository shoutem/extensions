{
  "name": "navigation",
  "version": "5.4.0",
  "platform": ">=13.0.7",
  "type": "system",
  "title": "Main Navigation",
  "releaseNotes": "* Improved user experience during tab navigation: The app will now retain the last opened screen when navigating between tab items. Additionally, if a user taps on an already active tab item, the app will open the first screen of that tab for a smoother interaction.",
  "description": "App-level navigation",
  "nativeChanges": {
    "lastVersion": "5.2.0"
  },
  "dependencies": [
    "shoutem.application",
    "shoutem.i18n",
    "shoutem.layouts",
    "shoutem.redux",
    "shoutem.theme"
  ],
  "categories": [],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "icon": "server/assets/add-main-navigation-image.png",
  "shortcuts": [
    {
      "name": "icons",
      "icon": "",
      "title": "Main navigation",
      "screen": "@.TabBar",
      "type": "navigation",
      "unique": true,
      "adminPages": [
        {
          "page": "shoutem.layouts.LayoutPage",
          "title": "Layout"
        },
        {
          "page": "@.IconsPage",
          "title": "Icons"
        },
        {
          "page": "@.NavigationBarPage",
          "title": "Navigation bar"
        }
      ]
    }
  ],
  "screens": [
    {
      "name": "TabBar",
      "title": "Tab bar",
      "image": "./server/assets/screens/tabbar.png",
      "groupTitle": "Choose navigation",
      "settingsPage": {
        "page": "@.TabBar"
      },
      "settings": {
        "startingScreen": null,
        "showIcon": true,
        "showText": true
      }
    },
    {
      "name": "Drawer",
      "title": "Drawer",
      "image": "./server/assets/screens/drawer.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.Drawer"
      },
      "settings": {
        "startingScreen": null,
        "showIcon": true,
        "showText": true
      }
    },
    {
      "name": "IconGrid",
      "title": "Icon grid",
      "image": "./server/assets/screens/icon.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.IconGrid"
      },
      "settings": {
        "iconSize": "medium",
        "scrolling": "paged",
        "gridAlignment": "bottomCenter",
        "cols": 3,
        "rows": 3,
        "parallaxEffect": "onDeviceOrientation",
        "tabletBackgroundImage": "",
        "backgroundImage": "",
        "backgroundWidth": 48,
        "backgroundHeight": 48,
        "textSize": "small"
      }
    },
    {
      "name": "List",
      "title": "List",
      "image": "./server/assets/screens/list.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.List"
      },
      "settings": {
        "backgroundImage": "",
        "tabletBackgroundImage": "",
        "parallaxEffect": "onScroll",
        "inItemAlignment": "center",
        "listAlignment": "top",
        "topOffset": 120,
        "showText": true,
        "showIcon": false,
        "itemHeight": 48,
        "textSize": "small"
      }
    },
    {
      "name": "TileGrid",
      "title": "Tile grid",
      "image": "./server/assets/screens/tile-grid.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.TileGrid"
      },
      "settings": {
        "backgroundImage": "",
        "parallaxEffect": "onScroll",
        "itemGutter": "small",
        "itemText": "topLeft",
        "textSize": "small"
      }
    },
    {
      "name": "CardList",
      "title": "Card list",
      "image": "./server/assets/screens/card-list.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.CardList"
      },
      "settings": {
        "backgroundImage": "",
        "parallaxEffect": "onScroll",
        "cardHeight": "medium",
        "itemGutter": "medium",
        "itemText": "topLeft",
        "isFullWidth": true,
        "textSize": "small"
      }
    },
    {
      "name": "None",
      "title": "None",
      "image": "./server/assets/screens/none.png",
      "extends": "@.TabBar",
      "settingsPage": {}
    }
  ],
  "pages": [
    {
      "name": "List",
      "type": "native-component"
    },
    {
      "name": "CardList",
      "type": "native-component"
    },
    {
      "name": "IconGrid",
      "type": "native-component"
    },
    {
      "name": "Drawer",
      "type": "native-component"
    },
    {
      "name": "TabBar",
      "type": "native-component"
    },
    {
      "name": "IconsPage",
      "type": "native-component"
    },
    {
      "name": "NavigationBarPage",
      "type": "native-component"
    },
    {
      "name": "TileGrid",
      "type": "native-component"
    }
  ],
  "extensionThemes": [
    {
      "name": "Theme",
      "path": "server/themes/theme.json"
    }
  ],
  "settingsPages": [
    {
      "page": "shoutem.theme.ExtensionThemeSettingsPage",
      "title": "Settings",
      "categories": [
        "style"
      ],
      "parameters": {
        "extensionTheme": "Theme"
      }
    }
  ],
  "settings": {
    "showTitle": true,
    "backgroundImageEnabledFirstScreen": false,
    "backgroundImage": null,
    "fitContainer": true,
    "uppercaseTitle": true
  }
}
