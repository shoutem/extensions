{
  "name": "navigation",
  "version": "1.8.3",
  "platform": "1.8.*",
  "type": "system",
  "title": "Main Navigation",
  "description": "App-level navigation",
  "categories": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "icon": "server/assets/add-main-navigation-image.png",
  "shortcuts": [
    {
      "name": "icons",
      "icon": "",
      "title": "Main navigation",
      "screen": "@.TabBar",
      "type": "navigation",
      "unique": true,
      "adminPages": [
        {
          "page": "shoutem.layouts.LayoutPage",
          "title": "Layout"
        },
        {
          "page": "@.IconsPage",
          "title": "Icons"
        },
        {
          "page": "@.NavigationBarPage",
          "title": "Navigation bar"
        }
      ]
    }
  ],
  "screens": [
    {
      "name": "TabBar",
      "title": "Tab bar",
      "image": "./server/assets/screens/tabbar.png",
      "groupTitle": "Choose navigation",
      "settingsPage": {
        "page": "@.TabBar"
      },
      "settings": {
        "startingScreen": null,
        "showIcon": true,
        "showText": true
      }
    },
    {
      "name": "Drawer",
      "title": "Drawer",
      "image": "./server/assets/screens/drawer.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.Drawer"
      },
      "settings": {
        "startingScreen": null,
        "showIcon": true,
        "showText": true
      }
    },
    {
      "name": "IconGrid",
      "title": "Icon grid",
      "image": "./server/assets/screens/icon.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.IconGrid"
      },
      "settings": {
        "iconSize": "medium",
        "scrolling": "paged",
        "gridAlignment": "bottomCenter",
        "cols": 3,
        "rows": 3,
        "parallaxEffect": "onDeviceOrientation",
        "tabletBackgroundImage": "",
        "backgroundImage": "",
        "backgroundWidth": 48,
        "backgroundHeight": 48
      }
    },
    {
      "name": "List",
      "title": "List",
      "image": "./server/assets/screens/list.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.List"
      },
      "settings": {
        "backgroundImage": "",
        "tabletBackgroundImage": "",
        "parallaxEffect": "onScroll",
        "inItemAlignment": "center",
        "listAlignment": "top",
        "topOffset": 120,
        "showText": true,
        "showIcon": false,
        "itemHeight": 48
      }
    },
    {
      "name": "TileGrid",
      "title": "Tile grid",
      "image": "./server/assets/screens/tile-grid.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.TileGrid"
      },
      "settings": {
        "backgroundImage": "",
        "parallaxEffect": "onScroll",
        "itemGutter": "small",
        "itemText": "topLeft"
      }
    },
    {
      "name": "CardList",
      "title": "Card list",
      "image": "./server/assets/screens/card-list.png",
      "extends": "@.TabBar",
      "settingsPage": {
        "page": "@.CardList"
      },
      "settings": {
        "backgroundImage": "",
        "parallaxEffect": "onScroll",
        "cardHeight": "medium",
        "itemGutter": "medium",
        "itemText": "topLeft",
        "isFullWidth": true
      }
    },
    {
      "name": "None",
      "title": "None",
      "image": "./server/assets/screens/none.png",
      "extends": "@.TabBar",
      "settingsPage": {}
    }
  ],
  "pages": [
    {
      "name": "List",
      "type": "native-component"
    },
    {
      "name": "CardList",
      "type": "native-component"
    },
    {
      "name": "IconGrid",
      "type": "native-component"
    },
    {
      "name": "Drawer",
      "type": "native-component"
    },
    {
      "name": "TabBar",
      "type": "native-component"
    },
    {
      "name": "IconsPage",
      "type": "native-component"
    },
    {
      "name": "NavigationBarPage",
      "type": "native-component"
    },
    {
      "name": "TileGrid",
      "type": "native-component"
    }
  ],
  "settings": {
    "showTitle": true,
    "backgroundImageEnabledFirstScreen": false,
    "backgroundImage": null,
    "fitContainer": true
  }
}
