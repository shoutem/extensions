      "title": "Website",
      "type": "string",
      "pattern": "^(https?|bundle|shoutem|tel|sms|mail|itms)://[-A-Za-z0-9+&@#/%?=~_|!:,.;]*[-A-Za-z0-9+&@#/%=~_|]$",
      "displayPriority": 4
    },
    "mail": {
      "format": "single-line",
      "title": "E-mail",
      "type": "string",
      "displayPriority": 5
    },
   
    "phone": {
      "format": "single-line",
      "title": "Phone",
      "type": "string",
      "displayPriority": 6
    },
    "twitter": {
