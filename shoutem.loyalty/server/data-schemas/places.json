    "linkedin": {
      "format": "uri",
      "title": "LinkedIn",
      "type": "string",
      "displayPriority": 11
    },
    "openingHours": {
      "format": "multi-line",
      "title": "Open Hours",
      "type": "string",
      "displayPriority": 12
    },
    "image": {
      "format": "attachment",
      "title": "Image",
      "type": "object",
      "referencedSchema": "shoutem.core.image-attachments",
      "displayPriority": 13
    }
