{
  "name": "places",
  "title":"Places",
  "properties": {
    "name": {
      "format": "single-line",
      "title": "Place name",
      "type": "string",
      "required":true,
      "displayPriority": 1
    },
    "location": {
      "format": "geolocation",
      "title": "Location",
