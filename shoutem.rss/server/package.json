{
  "name": "shoutem.rss",
  "version": "5.1.1-rc.0",
  "description": "Shoutem RSS extensions",
  "author": "Shoutem",
  "scripts": {
    "lint": "eslint --no-eslintrc -c .eslintrc src/**/*.{js,jsx}",
    "clean": "rimraf ./build/*",
    "build": "npm run clean && webpack --config ./bin/webpack.config.js",
    "watch": "npm run clean && webpack --watch --config ./bin/webpack.config.js",
    "serve": "webpack-dev-server --config ./bin/webpack.config.js",
    "dev": "run-p watch serve"
  },
  "dependencies": {
    "auto-bind": "^4.0.0",
    "dayjs": "^1.8.23",
    "i18next": "^19.8.2",
    "normalize-url": "^1.6.0",
    "urijs": "^1.18.9",
    "validator": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@shoutem/eslint-config-react": "1.0.10",
    "@shoutem/webpack-prepend-append": "^1.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.3.0",
    "css-loader": "3.6.0",
    "cssnano": "4.1.11",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.21.5",
    "file-loader": "6.2.0",
    "npm-run-all": "^4.1.5",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "sass": "1.70.0",
    "sass-loader": "10.5.2",
    "style-loader": "1.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "terser-webpack-plugin": "4.2.3",
    "url-loader": "3.0.0",
    "webpack": "4.47.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-modules-commonjs"
    ]
  }
}
