{
  "name": "sendbird",
  "title": "SendBird Chat",
  "description": "Connect your users with real-time chat!",
  "version": "6.0.2",
  "platform": ">=14",
  "releaseNotes": "No user facing changes\n\nFor developers\n* Added Node 18 support for server segment",
  "icon": "./server/assets/chat.png",
  "dependencies": [
    "shoutem.application",
    "shoutem.auth",
    "shoutem.camera",
    "shoutem.firebase",
    "shoutem.i18n",
    "shoutem.layouts",
    "shoutem.navigation",
    "shoutem.permissions",
    "shoutem.redux",
    "shoutem.social",
    "shoutem.theme"
  ],
  "categories": ["users"],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "shortcuts": [
    {
      "title": "Chat",
      "description": "Provide your users with means of direct interaction",
      "icon": "theme://activity.png",
      "name": "messageListScreen",
      "screen": "@.MessageListScreen"
    }
  ],
  "screens": [
    {
      "name": "MessageListScreen",
      "title": "List of previous conversations and option to start a new one"
    },
    {
      "name": "ChatWindowScreen",
      "settings": {
        "disableAdBanner": true
      }
    },
    {
      "name": "PhotoFullScreen"
    }
  ],
  "pages": [
    {
      "name": "ChatSettingsPage",
      "type": "react-page"
    }
  ],
  "settings": {
    "appId": "",
    "shoutemAppId": "0A41926F-F138-4AA1-9678-3C302A2F354C",
    "featureActive": false
  },
  "settingsPages": [
    {
      "page": "@.ChatSettingsPage",
      "title": "Settings"
    },
    {
      "page": "shoutem.theme.ExtensionThemeSettingsPage",
      "title": "Settings",
      "categories": ["style"],
      "parameters": {
        "extensionTheme": "Theme"
      }
    }
  ],
  "extensionThemes": [
    {
      "name": "Theme",
      "path": "server/themes/theme.json"
    }
  ]
}
