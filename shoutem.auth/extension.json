{
  "name": "auth",
  "version": "1.6.8",
  "platform": "1.6.*",
  "title": "Users",
  "icon": "server/assets/add-authentication-image.png",
  "description": "Show user profile, sign out user",
  "categories": [
    "users"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "screens": [
    {
      "name": "LoginScreen"
    },
    {
      "name": "RegisterScreen"
    },
    {
      "name": "UserProfileScreen"
    },
    {
      "name": "EditProfileScreen"
    },
    {
      "name": "MyProfileScreen"
    }
  ],
  "shortcuts": [
    {
      "title": "Logout",
      "description": "Allows user to logout from application",
      "name": "logoutAction",
      "action": "@.logoutAction"
    },
    {
      "title": "My profile",
      "description": "This screen lets users manage their user profile",
      "icon": "theme://profile.png",
      "name": "openMyProfile",
      "screen": "@.MyProfileScreen"
    }
  ],
  "settingsPages": [
    {
      "page": "@.UsersPage",
      "title": "Users"
    },
    {
      "page": "@.ProtectedScreensPage",
      "title": "Protected Screens"
    },
    {
      "page": "@.UserGroupsPage",
      "title": "Groups"
    },
    {
      "page": "@.GeneralSettingsPage",
      "title": "General"
    }
  ],
  "pages": [
    {
      "name": "GeneralSettingsPage",
      "type": "react-page"
    },
    {
      "name": "ProtectedScreensPage",
      "type": "react-page"
    },
    {
      "name": "UsersPage",
      "type": "react-page"
    },
    {
      "name": "UserGroupsPage",
      "type": "react-page"
    }
  ],
  "settings": {
    "authApiEndpoint": "auth.api.shoutem.com",
    "signupEnabled": true,
    "allScreensProtected": false,
    "providers": {
      "email": {
        "enabled": true
      },
      "facebook": {
        "enabled": false
      },
      "twitter": {
        "enabled": false
      }
    }
  }
}
